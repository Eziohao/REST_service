<!DOCTYPE html>
<html ng-app="ServiceApp">
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script type="text/javascript" src="../public/javascripts/controller.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.2/jquery.min.js"></script>
    <script src="../public/lib/angular.min.js"></script>
  </head>
  <body class="container">
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <div class="servicesChoose" ng-controller="formController">
    	<form class="servicesForm">
    		<select name="servicesSelect" id="servicesList">
    			<option ng-repeat="service in services" value="{{service.name}}"></option>
    		</select>
    	</form>
    </div>
  </body>
</html>
